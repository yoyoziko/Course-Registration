<style>
    body {
      font-family: "Poppins", sans-serif;
      transition: background-color .5s;
      
    }
    
    .sidenav {
      height: 100%;
      width: 0;
      position: fixed;
      z-index: 4;
      top: 0;
      left: 0;
      background-color: #070A16;
      overflow-x: hidden;
      transition: 0.5s;
      padding-top: 60px;
    }
    
    .sidenav a {
      padding: 8px 8px 8px 20px;
      text-decoration: none;
      font-size: 18px;
      color: #818181;
      display: block;
      transition: 0.3s;
    }
    
    .sidenav a:hover {
      color: #f1f1f1;
    }
    
    .sidenav .closebtn {
      position: absolute;
      top: 0;
      right: 5px;
      font-size: 36px;
      margin-left: 50px;
    }
    
  
    
    
   

    #main {
      transition: margin-left .5s;
      padding: 16px;
    }
    
    @media screen and (max-height: 450px) {
      .sidenav {padding-top: 15px;}
      .sidenav a {font-size: 18px;}
    }
    </style>
